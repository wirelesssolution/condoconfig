rule "OPEN Light"
when
Channel 'astro:sun:local:rise#start triggered START
then
    ulinealert.sendCommand("Sun Rise Start")
   //sendCommand(Light, OFF)
end

rule "OFF Light"
when
Channel 'astro:sun:local:set#start' triggered START
then
  ulinealert.sendCommand("Sun set Start")
//sendCommand(Light, ON)
end

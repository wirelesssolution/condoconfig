   { mqtt=">[mymqtt:cmnd/CAPTEUR_XXXXXX/POWER2:command:*:default],
   <[mymqtt:stat/CAPTEUR_XXXXXX/POWER2:state:default]" } 
   Switch   CAPTEUR_XXXXXX_POWER3        "Touch switch 3 Touch Gang 3"             <light>         (gLight)  [ "Lighting" ]          
   { mqtt=">[mymqtt:cmnd/CAPTEUR_XXXXXX/POWER3:command:*:default],
   <[mymqtt:stat/CAPTEUR_XXXXXX/POWER3:state:default]" } 

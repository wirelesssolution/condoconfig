   Switch   CAPTEUR_XXXXXX_POWER1 "Touch switch 3 Touch Gang 1"             <light>         (gLight)  [ "Lighting" ]          
   { mqtt=">[mymqtt:cmnd/CAPTEUR_XXXXXX/POWER1:command:*:default],
	 <[mymqtt:stat/CAPTEUR_XXXXXX/POWER1:state:default]" } 
